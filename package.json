{
  "name": "mountaineer-vim",
  "displayName": "mountainer.vim theme",
  "description": "A port of mountaineer.vim by TheNiteCoder",
  "version": "1.0.0",
  "publisher": "willfromspace",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Mountaineer Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/mountaineer.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./tsconfig.json",
    "watch": "tsc -watch -p ./tsconfig.json",
    "postinstall": "node ./out/extension.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/node": "^20",
    "@types/vscode": "^1.73.0",
    "eslint": "^9.13.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "keywords": [
    "vscode",
    "theme",
    "color-theme",
    "dark",
    "vim",
    "mountaineer",
    "colorscheme",
    "colors"
  ],
  "license": "MIT"
}
